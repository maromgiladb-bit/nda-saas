<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>{{doc_title}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <style>
    :root{
      --ink:#0a0e1a;
      --muted:#4b566b;
      --rule:#dde1e8;
      --rule-strong:#bfc5d1;
      --bg:#f4f6fa;
      --page-bg:#ffffff;
      --page-shadow:0 12px 48px rgba(8,15,35,.1);
      --sans: ui-sans-serif, -apple-system, "Segoe UI", Roboto, Arial, "Helvetica Neue", sans-serif;
      --serif: "Georgia Pro", Georgia, "Times New Roman", Times, serif;
      --highlight:#fff8d1;
      --accent:#2563eb;
      --accent-dark:#1e40af;
      --soft-bg:#fafbfc;
    }

    /* Page */
    html,body{margin:0;padding:0;background:var(--bg);color:var(--ink);}
    .page{
      box-sizing:border-box;
      margin:32px auto;
      background:var(--page-bg);
      box-shadow:var(--page-shadow);
      border:1px solid var(--rule);
      border-radius:16px;
      max-width:900px;
      padding:32mm 26mm;
      font-family: var(--serif);
      font-size:12.5pt;
      line-height:1.75;
    }

    /* Typography */
    h1{
      font-family: var(--sans);
      font-size:22pt;
      font-weight:800;
      letter-spacing:.06em;
      text-transform:uppercase;
      text-align:center;
      margin:0 0 16mm;
      padding-bottom:8mm;
      border-bottom:3px solid var(--accent);
      color:var(--accent-dark);
      position:relative;
    }
    h1:after{
      content:"";
      position:absolute;
      bottom:-3px;
      left:50%;
      transform:translateX(-50%);
      width:120px;
      height:3px;
      background:var(--accent);
      box-shadow:0 0 20px rgba(37,99,235,.3);
    }

    .lead{
      font-size:12.5pt;
      margin:0 0 9mm;
      color:var(--ink);
      text-align:justify;
      line-height:1.85;
      hyphens:auto;
    }
    .smallcaps{ 
      font-variant:small-caps; 
      letter-spacing:.05em;
      font-weight:600;
      color:var(--accent-dark);
    }
    .def{ 
      font-style:italic;
      font-weight:500;
      color:#1a1f2e;
    }

    .section{
      margin:12mm 0;
      padding-top:3mm;
      page-break-inside:avoid;
    }
    .section h2{
      font-family: var(--sans);
      font-size:14pt;
      margin:0 0 5mm;
      font-weight:800;
      display:flex;
      align-items:center;
      gap:12px;
      letter-spacing:.02em;
      color:var(--accent-dark);
      text-decoration:underline;
      text-decoration-color:var(--accent);
      text-decoration-thickness:2.5px;
      text-underline-offset:4px;
      padding-bottom:2mm;
      border-bottom:1px solid var(--rule);
    }
    .num{
      display:inline-flex;
      align-items:center;
      justify-content:center;
      min-width:32px;
      height:32px;
      padding:0 10px;
      border:2px solid var(--accent);
      border-radius:50%;
      font-family: var(--sans);
      font-size:12pt;
      font-weight:700;
      background:linear-gradient(135deg, #eff6ff 0%, #dbeafe 100%);
      color:var(--accent-dark);
      line-height:1;
      box-shadow:0 2px 8px rgba(37,99,235,.15);
      text-decoration:none;
    }

    .clause{
      margin:5mm 0;
      text-align:justify;
      line-height:1.85;
      hyphens:auto;
    }
    .indent{
      margin:5mm 0;
      line-height:1.85;
      text-align:justify;
      padding-left:10mm;
      border-left:3px solid var(--accent);
      background:linear-gradient(90deg, rgba(37,99,235,.04) 0%, transparent 100%);
      padding-top:3mm;
      padding-bottom:3mm;
      padding-right:4mm;
    }

    /* Tables */
    .table{
      width:100%;
      border-collapse:separate;
      border-spacing:0;
      margin:10mm 0;
      border:2px solid var(--rule-strong);
      border-radius:12px;
      overflow:hidden;
      box-shadow:0 4px 16px rgba(0,0,0,.06);
    }
    .table th, .table td{
      padding:12px 16px;
      vertical-align:top;
      border-bottom:1px solid var(--rule);
      border-right:1px solid var(--rule);
    }
    .table th:last-child, .table td:last-child{ border-right:none; }
    .table tr:last-child td{ border-bottom:none; }
    .table th{
      background:linear-gradient(180deg, #f1f5f9 0%, #e2e8f0 100%);
      font-family: var(--sans);
      text-transform:uppercase;
      letter-spacing:.04em;
      font-weight:700;
      font-size:11.5pt;
      color:var(--accent-dark);
      border-bottom:2px solid var(--accent);
    }
    .table tbody tr{
      transition:background .2s ease;
    }
    .table tbody tr:hover{
      background:#fafbfc;
    }
    .table tbody tr:nth-child(even) td{
      background:var(--soft-bg);
    }
    .table strong{
      color:var(--accent-dark);
      font-weight:600;
    }

    hr{
      border:none;
      height:3px;
      background:linear-gradient(90deg, transparent, var(--accent), transparent);
      margin:14mm 0 10mm;
    }

    /* Signature block */
    .sign{
      margin-top:14mm;
      display:grid;
      grid-template-columns:1fr 1fr;
      gap:28mm;
    }
    .sign > div{
      border:2px solid var(--rule-strong);
      border-radius:10px;
      padding:16px;
      background:var(--soft-bg);
      box-shadow:0 2px 8px rgba(0,0,0,.04);
    }
    .line{
      border-bottom:2px solid var(--accent-dark);
      height:0;
      margin:10mm 0 4mm;
    }
    .muted{ 
      color:var(--muted); 
      font-size:11.5pt;
      font-family:var(--sans);
    }
    .center{ text-align:center; }
    .nowrap{ white-space:nowrap; }

    /* Fillable fields */
    .field{
      background:var(--highlight);
      padding:2px 6px;
      border-radius:4px;
      box-shadow:inset 0 -2px 0 rgba(0,0,0,.08);
      font-weight:600;
      border:1px solid rgba(245,158,11,.2);
      color:#854d0e;
    }
    /* Placeholder look when value is missing */
    .field.placeholder{
      color:#6b7280;        /* muted gray */
      font-style:italic;    /* optional */
    }

    /* Print refinements */
    @media print{
      body{ background:#fff; }
      .page{
        box-shadow:none;
        margin:0;
        border:none;
        border-radius:0;
        padding:24mm 20mm;
      }
      .field{
        background:transparent;
        box-shadow:none;
        border:0;
        border-bottom:1px solid #999;
        color:inherit;
      }
      h1{
        border-bottom-color:#000;
        color:#000;
      }
      h1:after{ display:none; }
      h2{
        color:#000;
        text-decoration-color:#000;
        border-bottom-color:#ccc;
      }
      .num{
        border-color:#000;
        background:#fff !important;
        box-shadow:none;
        color:#000;
        -webkit-print-color-adjust:exact;
        print-color-adjust:exact;
      }
      .indent{
        border-left-color:#000;
        background:transparent;
      }
      .table{
        border-color:#999;
        box-shadow:none;
      }
      .table th{
        background:#eee !important;
        border-bottom-color:#000;
        color:#000;
        -webkit-print-color-adjust:exact;
        print-color-adjust:exact;
      }
      .table th, .table td{ border-color:#999; }
      .table tbody tr:nth-child(even) td{ background:#f9f9f9 !important; }
      .table tbody tr:hover{ background:transparent; }
      .sign > div{
        border-color:#999;
        background:transparent;
        box-shadow:none;
      }
      .line{ border-bottom-color:#000; }
      hr{ background:#000; }
      .section, .clause, .lead{ orphans:3; widows:3; }
      h2{ page-break-after:avoid; }
    }
    /* ===== Design polish pack: non-breaking, CSS-only ===== */

/* Tighter measure + improved rhythm */
.page{
  max-width: 880px;              /* slightly narrower for easier reading */
  padding: 30mm 24mm;            /* balanced margins */
  letter-spacing: 0.001em;       /* micro tracking for long text blocks */
}

/* Title: reduce glow, add subtle top rule echo */
h1{
  font-weight: 900;
  text-shadow: none;
  padding-bottom: 7mm;
}
h1:after{
  width: 96px;                   /* more discreet center accent */
  height: 2px;
  box-shadow: none;
}

/* Body copy refinements */
.lead, .clause{
  font-size: 12.25pt;            /* hair smaller for dense pages */
  line-height: 1.82;
  hanging-punctuation: first;    /* nicer opening quotes if supported */
  text-underline-offset: 3px;
}
.smallcaps{
  font-variant-caps: small-caps; /* stronger small caps hint */
}

/* Section headings: calmer underline, better spacing */
.section{ margin: 11mm 0; }
.section h2{
  text-decoration-thickness: 2px;
  border-bottom-color: var(--rule-strong);
  padding-bottom: 6px;
}

/* Number badge: lighter, more typographic */
.num{
  min-width: 28px;
  height: 28px;
  font-size: 11pt;
  border-radius: 999px;
  border-width: 1.5px;
  background: #f5f8ff;
  color: var(--accent-dark);
  box-shadow: 0 1px 4px rgba(37,99,235,.08);
}

/* Indented lists: crisper lane with softer bg */
.indent{
  border-left-width: 2px;
  border-left-color: var(--rule-strong);
  background: linear-gradient(90deg, rgba(8,15,35,.025) 0%, transparent 100%);
  padding-left: 9mm;
  padding-top: 2mm;
  padding-bottom: 2mm;
}

/* Field chips: less saturation, better contrast + inline baseline */
.field{
  background: #fff7c2;
  color: #6b4e12;
  border-color: rgba(0,0,0,.08);
  padding: 1px 4px;
  box-shadow: inset 0 -1px 0 rgba(0,0,0,.07);
  border-radius: 3px;
  vertical-align: baseline;
}

/* Tables: calmer zebra, subtle inner shadows, compact cells */
.table{
  border-width: 1px;
  border-color: var(--rule-strong);
  box-shadow: 0 2px 12px rgba(0,0,0,.05);
}
.table th, .table td{ padding: 10px 14px; }
.table tbody tr:nth-child(even) td{ background: #f7f9fc; }
.table tbody tr:hover{ background: #f3f6fb; }

/* Signature cards: align baselines, add label styling */
.sign{
  gap: 22mm;
}
.sign > div{
  border: 1px solid var(--rule-strong);
  background: #fbfcfe;
}
.sign .clause{ margin: 0; }
.sign .muted{
  font-size: 11pt;
  color: #5b667b;
}
.line{
  border-bottom-width:1.5px;
  margin: 8mm 0 3mm;
}

/* Horizontal rule: subtler gradient */
hr{
  height: 2px;
  margin: 12mm 0 9mm;
  background: linear-gradient(90deg, transparent, var(--rule-strong), transparent);
}

/* Print: tighten color usage for better legibility on B&W */
@media print{
  .page{ padding: 24mm 18mm; }
  .lead, .clause{ line-height: 1.75; }
  .table tbody tr:nth-child(even) td{ background: #f2f2f2 !important; }
  .table tbody tr:hover{ background: transparent; }
}

/* Optional: compact page variant (add class="page page--compact" if needed) */
.page.page--compact{
  max-width: 820px;
  padding: 26mm 22mm;
  font-size: 12pt;
  line-height: 1.78;
}

/* Optional: dark mode (respects system preference, screen only) */
@media (prefers-color-scheme: dark){
  :root{
    --bg:#0b1220;
    --page-bg:#0f172a;
    --ink:#e5e7eb;
    --muted:#9aa3b2;
    --rule:#1f2a44;
    --rule-strong:#2a3552;
    --accent:#60a5fa;
    --accent-dark:#93c5fd;
    --soft-bg:#0b1326;
  }
  body{ background: var(--bg); }
  .page{
    background: var(--page-bg);
    border-color: var(--rule);
    box-shadow: 0 12px 48px rgba(0,0,0,.45);
  }
  h1, .section h2{ color: var(--accent-dark); }
  .num{ background:#0b1a33; color:#dbeafe; border-color:#27406b; }
  .indent{ background: linear-gradient(90deg, rgba(96,165,250,.08) 0%, transparent 100%); }
  .table th{ background:#0e1a33; color:#e5e7eb; border-bottom-color:#2a3552; }
  .table, .table th, .table td{ border-color:#2a3552; }
  .table tbody tr:nth-child(even) td{ background:#0c162b; }
  .sign > div{ background:#0b1326; border-color:#2a3552; }
}

/* Optional: watermark when rendering a draft (no content changes) */
/* Add {{#if is_draft}} body has class "is-draft" {{/if}} if you like */
body.is-draft .page::before{
  content: "DRAFT";
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%) rotate(-18deg);
  font-family: var(--sans);
  font-size: 120px;
  letter-spacing: .2em;
  color: rgba(30,41,59,.08);
  pointer-events: none;
  z-index: 0;
}
.page{ position: relative; z-index: 1; }

  </style>
